<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping
    xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="LundSite\Entity\VideoTestimonials" table="video_testimonials">
        <indexes>
            <index name="videoTestimonialsId" columns="video_testimonials_id"/>
            <index name="status" columns="status"/>
        </indexes>
        <id name="videoTestimonialsId" column="video_testimonials_id"  type="integer">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="createdAt" column="created_at" type="datetime" nullable="true"/>
        <field name="createdBy" column="created_by" type="string" length="255" nullable="true"/>
        <field name="modifiedAt" column="modified_at" type="datetime" nullable="true"/>
        <field name="modifiedBy" column="modified_by" type="string" length="255" nullable="true"/>
        <field name="deleted" type="boolean" nullable="true"/>
        <field name="status" column="status" type="string" length="255" nullable="true"/>
        <field name="title" column="title" type="string" length="255" nullable="true"/>
        <field name="comment" type="text" nullable="true"/>
        <field name="url" column="url" type="string" length="255" nullable="true"/>
        <many-to-one field="productLine" target-entity="LundProducts\Entity\ProductLines" fetch="EAGER">
            <join-column name="product_line_id" referenced-column-name="product_line_id"/>
        </many-to-one>
        <many-to-one field="ecomCustomer" target-entity="RocketEcom\Entity\EcomCustomer" fetch="EAGER">
            <join-column name="ecom_customer_id" referenced-column-name="ecom_customer_id"/>
        </many-to-one>
    </entity>
</doctrine-mapping>
