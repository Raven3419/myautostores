<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping
    xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="LundSite\Entity\Faq" table="faq">
        <indexes>
            <index name="faqId" columns="faq_id"/>
            <index name="deleted" columns="deleted"/>
        </indexes>
        <id name="faqId" column="faq_id"  type="integer">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="createdAt" column="created_at" type="datetime" nullable="true"/>
        <field name="createdBy" column="created_by" type="string" length="255" nullable="true"/>
        <field name="modifiedAt" column="modified_at" type="datetime" nullable="true"/>
        <field name="modifiedBy" column="modified_by" type="string" length="255" nullable="true"/>
        <field name="deleted" type="boolean" nullable="true"/>
        <field name="disabled" type="boolean" nullable="true"/>
        <many-to-one field="site" target-entity="RocketCms\Entity\Site" fetch="EAGER">
            <join-column name="site_id" referenced-column-name="site_id"/>
        </many-to-one>
        <field name="question" type="text" nullable="true"/>
        <field name="answer" type="text" nullable="true"/>
        <many-to-one field="brand" target-entity="LundProducts\Entity\Brands" fetch="EAGER">
            <join-column name="brand_id" referenced-column-name="brand_id"/>
        </many-to-one>
        <many-to-one field="faqHeaders" target-entity="LundSite\Entity\FaqHeaders" fetch="EAGER">
            <join-column name="faq_headers_id" referenced-column-name="faq_headers_id"/>
        </many-to-one>
    </entity>
</doctrine-mapping>
